     <h1>Welcome to my Web Form</h1>
    <h3>A Demo form for hotel reservation.</h3>

    <form method="post" action="form.aspx">

    <fieldset>
        <legend>Customer Information</legend> 
        <p><label>Fullname: <input type="text" id="customerName"name="customerName" maxlength="12"/></label></p>
        <p><label>Email address: <input type="text" id="email" name="email" maxlength="20"</label></p>
        <p><label>Phone <input type="text" id="phone" name="phone" maxlength="10"</label></p>
    </fieldset>

    <fieldset>
       <legend>Room Information</legend>
       <p><label>Arrival Date: <input type="text" id="arrivaldate" name="arrivaldate" maxlength="8"</label></p>
       <p><label>Arrival Time: <input type="text" id="arrivaltime" name="arrivaltime" maxlength="8"</label></p>

    </fieldset>

    <fieldset>
        <legend>Other Information</legend>
        <p><label>Promo Code: <input type="text" id="promocode" name="promocode" maxlength="4"</label></p>
        <p><label>Special Notes:</label></p>
        <textarea name="Specialnotes:" cols="50" rows="5" maxlength="1000"></textarea> 
    </fieldset>

    </form>



   
